<!DOCTYPE html>
<html>
  <head>
    <title>Project1</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    t "views.layouts.application", media: "all", "data-turbolinks-track": "reload" %>
    <%= javascript_include_tag t "views.layouts.application", "data-turbolinks-track": "reload" %>
  </head>
  <body>
  	<%= render "layouts/header" %>
    <div class="container">
      <% flash.each do |message_type, message| %>
        <div class="alert alert-<%= message_type %>"><%= message %></div>
      <% end %>
    </div>
    <%= yield %>
</html>
